{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "My Extension",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "alarms",
    "background"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net; object-src 'self'"
}
