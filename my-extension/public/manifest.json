{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "My Extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "alarms",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://cdn.jsdelivr.net/"]
}
